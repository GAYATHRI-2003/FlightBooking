
<!DOCTYPE html>
<html lang="en">
  <head>
    <title>Profile</title>
    <link rel="stylesheet" href="css/home.css" />
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.1.1/css/all.min.css">
   
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css">
  </head>
  <body>
    
    <div class="header">
       
        <nav class="navbar">
            <ul class="link-group">
                <li class="link active"><a href="home.html">Home</a></li>
                <li class="link"><a href="search.html">Search</a></li>
                <li class="link"><a href="/bookings">My Bookings</a></li>
                <li class="link"><a href="#"><i class='fa fa-user' id="profile"></i></a></li>
            </ul>
        </nav>
    
    </div>
    <div class="image">
      <img src="images/back.jpg">
    </div>
    <div class="profdetails">
        
            <img src="images/user-icon.png" alt="" class="prof">
            <div class="user">
              <% usersList.forEach( user => {%>
             <form action="logo" method="post">
            <p><i class="fas fa-user"></i>&nbsp<span><%= user.name%></span> </p>
            <br>
            <p><i class="fas fa-phone"></i>&nbsp<span><%= user.phone%></span></p>
            <br>
            <p><i class="fas fa-envelope"></i>&nbsp<span><%= user.email%></span></p>

            <br>
            <a href="/logout" class="btn">Logout</a>
            </form>
        <%})%>
         </div>

        </div>
    </body>
    </html>